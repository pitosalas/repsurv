<%= content_tag :tr do %>
  <%= content_tag :td do %>
    <%= Question.find(value.question_id).text %>
  <% end %>

  <% 6.times do |answer| %>
    <%= content_tag :td do %>
      <% name = "Q#{value.question_id}" %>
      <%= radio_button_tag name, "#{answer}" %>
    <% end %>
  <% end %>
<% end %>
