<% if @program.errors.any? %> 
  <div id="error_explanation">
    <div class="alert alert-error">
      The form contains <%= pluralize(@program.errors.count, "error") %>.
    </div>
    <ul>
      <% @program_id.errors.full_messages.each do |msg| %>
        <li>* <%= msg %></li>
      <% end %>
    </ul>
  </div> 
<% end %>